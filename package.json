{
    "name": "scapestory-server",
    "version": "1.0.0",
    "description": "",
    "main": "dist/bundle.js",
    "scripts": {
        "preinstall": "npx npm-force-resolutions",
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch:server": "nodemon \"./dist/bundle.js\" --watch \"./dist\" ",
        "watch:build": "webpack --watch",
        "dev": "npm run db:migrate && npm-run-all --parallel watch:server watch:build",
        "start": "npm run db:migrate && node dist/bundle.js",
        "build": "webpack --config webpack.config.production.ts",
        "seed": "ts-node -r tsconfig-paths/register src/db/seed/index.ts",
        "seed-production": "ts-node -r tsconfig-paths/register src/db/seed/production.ts",
        "lint": "tslint ./src/**/*.ts",
        "db:migrate": "sequelize db:migrate --config ./src/db/sequelize/config --migrations-path ./src/db/sequelize/migrations --debug",
        "db:migrate:undo": "sequelize db:migrate:undo --config ./src/db/sequelize/config --migrations-path ./src/db/sequelize/migrations --debug",
        "codegen": "graphql-codegen --config codegen.json",
        "pretty": "prettier --write \"./**/*.{ts,json}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://davinci2015@bitbucket.org/scapestory/scapestory.git"
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://bitbucket.org/scapestory/scapestory#readme",
    "engines": {
        "node": "14.x"
    },
    "resolutions": {
        "**/**/fs-capacitor": "^6.2.0",
        "**/graphql-upload": "^11.0.0"
    },
    "devDependencies": {
        "@graphql-codegen/add": "^2.0.2",
        "@graphql-codegen/cli": "^1.20.1",
        "@graphql-codegen/introspection": "1.18.1",
        "@graphql-codegen/typescript": "1.21.0",
        "@graphql-codegen/typescript-operations": "1.17.14",
        "@graphql-codegen/typescript-react-apollo": "2.2.1",
        "@graphql-codegen/typescript-resolvers": "1.18.2",
        "@types/bcrypt": "^3.0.0",
        "@types/dotenv": "^6.1.1",
        "@types/faker": "^5.1.6",
        "@types/graphql": "^14.5.0",
        "@types/graphql-fields": "^1.3.3",
        "@types/node": "^14.14.27",
        "@types/passport": "^1.0.6",
        "@types/passport-facebook-token": "^0.4.34",
        "@types/uuid": "^8.3.0",
        "@types/validator": "^13.1.3",
        "@types/yup": "^0.29.11",
        "@typescript-eslint/eslint-plugin": "^4.15.0",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "prettier": "2.2.1",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.4.2",
        "@typescript-eslint/parser": "^4.15.0",
        "apollo-server": "^2.21.0",
        "bcrypt": "^5.0.0",
        "cloudinary": "^1.24.0",
        "dataloader": "^2.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.20.0",
        "faker": "^5.4.0",
        "graphql": "^15.5.0",
        "graphql-fields": "^2.0.3",
        "graphql-modules": "^1.2.1",
        "graphql-tag": "^2.11.0",
        "graphql-upload": "^11.0.0",
        "jwt-simple": "^0.5.6",
        "passport": "^0.4.1",
        "passport-facebook-token": "^4.0.0",
        "passport-google-token": "^0.1.2",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.5.0",
        "sequelize-cli": "^6.2.0",
        "sequelize-typescript": "^2.0.0",
        "slugify": "^1.4.6",
        "ts-loader": "^8.0.17",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.3.0",
        "typescript": "^4.1.5",
        "uuid": "^8.3.2",
        "webpack": "^5.21.2",
        "webpack-node-externals": "^2.5.2",
        "winston": "^3.3.3",
        "yup": "^0.32.8"
    }
}
