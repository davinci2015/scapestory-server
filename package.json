{
    "name": "scapestory-server",
    "version": "1.0.0",
    "description": "",
    "main": "dist/bundle.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch:server": "nodemon \"./dist/bundle.js\" --watch \"./dist\" ",
        "watch:build": "webpack --watch",
        "dev": "npm run db:migrate && npm-run-all --parallel watch:server watch:build",
        "start": "npm run db:migrate && node dist/bundle.js",
        "build": "webpack --config webpack.config.production.ts",
        "seed": "ts-node -r tsconfig-paths/register src/db/seed/index.ts",
        "seed-production": "ts-node -r tsconfig-paths/register src/db/seed/production.ts",
        "lint": "tslint ./src/**/*.ts",
        "db:migrate": "sequelize db:migrate --config ./src/db/sequelize/config --migrations-path ./src/db/sequelize/migrations --debug",
        "db:migrate:undo": "sequelize db:migrate:undo --config ./src/db/sequelize/config --migrations-path ./src/db/sequelize/migrations --debug",
        "codegen": "graphql-codegen --config codegen.json",
        "pretty": "prettier --write \"./**/*.{ts,json}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://davinci2015@bitbucket.org/scapestory/scapestory.git"
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://bitbucket.org/scapestory/scapestory#readme",
    "devDependencies": {
        "@graphql-codegen/add": "^1.8.3",
        "@graphql-codegen/cli": "^1.8.3",
        "@graphql-codegen/introspection": "1.4.0",
        "@graphql-codegen/typescript": "1.4.0",
        "@graphql-codegen/typescript-operations": "1.4.0",
        "@graphql-codegen/typescript-react-apollo": "1.4.0",
        "@graphql-codegen/typescript-resolvers": "1.4.0",
        "@types/bcrypt": "^3.0.0",
        "@types/bluebird": "^3.5.29",
        "@types/dotenv": "^6.1.1",
        "@types/faker": "^4.1.7",
        "@types/graphql": "^14.5.0",
        "@types/graphql-fields": "^1.3.2",
        "@types/node": "^11.15.3",
        "@types/passport": "^1.0.1",
        "@types/passport-facebook-token": "^0.4.33",
        "@types/uuid": "^3.4.6",
        "@types/validator": "^12.0.1",
        "@types/yup": "^0.26.24",
        "@typescript-eslint/eslint-plugin": "^2.8.0",
        "nodemon": "^1.19.4",
        "npm-run-all": "^4.1.5",
        "prettier": "1.19.1",
        "sequelize-cli": "^5.5.1",
        "webpack-cli": "^3.3.10",
        "webpack-graphql-loader": "^1.0.2"
    },
    "dependencies": {
        "@graphql-modules/core": "^0.7.13",
        "@material-ui/core": "^4.6.0",
        "@sendgrid/mail": "^6.5.0",
        "@typescript-eslint/parser": "^2.8.0",
        "apollo-server": "^2.9.7",
        "bcrypt": "^3.0.7",
        "cloudinary": "^1.17.0",
        "dataloader": "^1.4.0",
        "dotenv": "^7.0.0",
        "eslint": "^6.6.0",
        "faker": "^4.1.0",
        "graphql": "^14.5.8",
        "graphql-fields": "^2.0.3",
        "graphql-tag": "^2.10.1",
        "jwt-simple": "^0.5.6",
        "passport": "^0.4.0",
        "passport-facebook-token": "^3.3.0",
        "passport-google-token": "^0.1.2",
        "pg": "^7.12.1",
        "pg-hstore": "^2.3.3",
        "react-transition-group": "^4.3.0",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^5.21.2",
        "sequelize-typescript": "^1.0.0",
        "slugify": "^1.3.6",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.4.1",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.7.2",
        "uuid": "^3.3.3",
        "webpack": "^4.41.2",
        "webpack-node-externals": "^1.7.2",
        "winston": "^3.2.1",
        "yup": "^0.27.0"
    }
}
